{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "array-element-newline": [
            "error",
            "never"
        ],
        "indent": [
            "error",
            4
        ],
        "semi": [
            "error",
            "never"
        ]
    }
}